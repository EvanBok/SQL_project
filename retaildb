CREATE TABLE finance (
	product_id VARCHAR(50),
	listing_price REAL, sale_price REAL, discount REAL, revenue REAL, modified_listing_price REAL, modified_sale_price REAL, modified_discount REAL, modified_revenue REAL);
CREATE TABLE info (
	product_name VARCHAR(64),
	product_id VARCHAR(50),
	description VARCHAR(512)
, modified_product_name VARCHAR, modified_description VARCHAR);
CREATE UNIQUE INDEX info_product_id_IDX ON info (product_id);
CREATE UNIQUE INDEX finance_product_id_IDX ON finance (product_id);
CREATE TABLE brands (
	product_id VARCHAR(50),
	brand VARCHAR(50)
, modified_brand VARCHAR);
CREATE TABLE reviews (
	product_id VARCHAR(50),
	rating VARCHAR(50),
	reviews VARCHAR(50),
	"Hour" REAL,
	"minute" REAL,
	real_rating REAL,
	real_reviews REAL,
	"Unnamed: 7" VARCHAR(50)
);
CREATE TABLE traffic (
	product_id VARCHAR(50),
	last_visited TEXT(50)
, modified_last_visited TEXT);

